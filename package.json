{
  "name": "nodejs-sector-alarm",
  "version": "2.0.8",
  "description": "NodeJS client for Sector Alarm",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "JÃ¸rgen Grimnes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jorgenkg/nodejs-sector-alarm.git"
  },
  "scripts": {
    "prepare": "npx husky install && npx patch-package",
    "test": "npx tape --enable-source-maps --unhandled-rejections=strict dist/test/integration/*.js",
    "dist": "rm -rf dist/ && npx tsc --build tsconfig.prod.json && rm dist/lib/@types/*.js",
    "clean": "rm -rf dist/",
    "release": "npm run clean && npm run dist && npm run test && standard-version -a",
    "generate-docs": "npx typedoc --excludePrivate --hideGenerator --includeVersion --readme ./README.md --out docs index.ts lib"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "got": "^11.8.2",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "13.1.0",
    "@sinonjs/fake-timers": "7.1.2",
    "@types/debug": "4.1.7",
    "@types/got": "9.6.12",
    "@types/koa": "2.13.4",
    "@types/koa-bodyparser": "4.3.2",
    "@types/koa-route": "3.2.4",
    "@types/node": "16.4.10",
    "@types/tape": "4.13.2",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "changelog-parser": "^2.8.0",
    "commitlint": "13.1.0",
    "eslint": "7.32.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "husky": "7.0.1",
    "koa": "2.13.1",
    "koa-bodyparser": "4.3.0",
    "koa-route": "3.2.0",
    "patch-package": "^6.4.7",
    "standard-version": "9.3.1",
    "tape": "5.3.0",
    "typedoc": "^0.21.5",
    "typescript": "4.3.5",
    "why-is-node-running": "2.2.0"
  },
  "keywords": [
    "sectoralarm",
    "sector alarm",
    "sector-alarm",
    "sector"
  ],
  "engines": {
    "node": ">=12.0"
  },
  "files": [
    "dist/lib",
    "dist/index.*"
  ]
}
